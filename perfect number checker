#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n;
    printf("Enter a number: ");
    scanf("%d",&n);

    int sum = 0;

    for(int i=1; i<n; i++)
    {
        if(n%i==0)
        {
          sum += i;
        }
    }

    if(sum==n)
    {
        printf("Yes");
    }
    else
    {
        printf("No");
    }

    return 0;
}
